{
  "private": true,
  "name": "@vega/dynamic-grid",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "pnpm glob && vite --strictPort --port 5010",
    "fix:code": "eslint --fix --quiet -c .eslintrc.cjs src",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "format:check": "prettier --check src/**/*.{ts,tsx}",
    "glob": "run-p glob:*",
    "glob:fonts": "cp -R ./node_modules/@nexus/styles/dist/fonts public",
    "glob:icons": "cp -R ./node_modules/@nexus/styles/dist/icons public",
    "glob:locales": "node i18n-glob.mjs",
    "glob:styles": "cp -R ./node_modules/@nexus/styles/dist/styles public",
    "lint": "run-s lint:code lint:report",
    "lint:code": "eslint --quiet -c .eslintrc.cjs src",
    "lint:report": "eslint --format checkstyle --output-file eslintreport.xml -c .eslintrc.cjs src",
    "prepare": "husky",
    "prettier:package-json": "prettier-package-json --write ./package.json",
    "preview": "vite preview --host --strictPort --port 5010",
    "refresh": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "serve": "run-s build preview",
    "start": "pnpm dev",
    "test": "vitest run --passWithNoTests",
    "test:ui": "vitest --ui --open --passWithNoTests",
    "test:watch": "vitest --watch --passWithNoTests"
  },
  "peerDependencies": {
    "@nexus/apis": "latest",
    "@nexus/charts": "latest",
    "@nexus/grids": "latest",
    "@nexus/i18n": "latest",
